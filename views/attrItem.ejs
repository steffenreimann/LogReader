<div class=" input-group mb-2">
    <input onchange="changeAttr('<%- data.id %>')" data-id="<%- data.id %>" id="attrInput-<%- data.id %>" type="filter" class="form-control btn-outline-light text-light bg-dark" value="<%- data.value %>" aria-label="Search" data-mdb-ripple-color="dark" />
    <button onclick="removeAttr('<%- data.id %>')" data-id="<%- data.id %>" class="btn btn-dark btn-outline-danger" type="button" data-ripple-color="dark">
        <i class="fas fa-times"></i>
    </button>
</div>