<div class="col-md d-flex input-group w-auto">
    <input onchange="changeAttr('<%- data.id %>')" data-id="<%- data.id %>" id="attrInput-<%- data.id %>" type="filter" class="form-control" value="<%- data.value %>" aria-label="Search" />
    <button onclick="removeAttr('<%- data.id %>')" data-id="<%- data.id %>" class="btn btn-outline-primary" type="button" data-ripple-color="dark">
        x
    </button>
</div>